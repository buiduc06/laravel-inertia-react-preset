{
    "name": "ravenberg/laravel-inertia-react-preset",
    "description": "An Inertia.js + Reactjs preset for the Laravel Framework",
    "keywords": ["laravel", "inertia", "react", "tailwindcss", "preset"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Ravenberg",
            "email": "lee@ravenberg.dev"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/console": "^6.5",
        "illuminate/filesystem": "^6.5",
        "illuminate/support": "^6.5",
        "inertiajs/inertia-laravel": "^0.1",
        "laravel/ui": "^1.0@dev"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ravenberg\\InertiaReactPreset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ravenberg\\InertiaReactPreset\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ravenberg\\InertiaReactPreset\\InertiaReactPresetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
